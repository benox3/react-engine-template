    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "body-parser": "^1.13.2",
    "cookie-parser": "^1.3.5",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "express": "^4.13.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "history": "^2.0.2",
    "json-loader": "^0.5.4",
    "morgan": "^1.6.1",
    "node-jsx": "^0.13.3",
    "react": "^0.14.8",
    "react-engine": "^3.2.1",
    "react-router": "^2.2.4",
    "serve-favicon": "^2.3.0",
    "webpack": "^1.13.0"
  },
  "devDependencies": {
    "babel-preset-react-hmre": "^1.1.1",
    "nodemon": "^1.9.1",
    "webpack-dev-server": "^1.14.1"
